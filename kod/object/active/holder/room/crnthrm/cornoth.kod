   RecalcLightAndWeather()
   {
      local iGroup;

      iGroup = 1 + (Send(SYS,@GetHour) % 12);

      Send(self,@AnimateWall,#wall=CLOCK_FACE,#animation=ANIMATE_NONE,
            #first_group=iGroup);

      propagate;
   }

