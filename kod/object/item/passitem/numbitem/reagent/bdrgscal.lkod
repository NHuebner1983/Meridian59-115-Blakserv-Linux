bluedragonscale_name_rsc = de "Die Drachenschuppe"
bluedragonscale_desc_rsc = de \
   "Diese Drachenschuppe ist so hart wie Jasper-Fels und so dick wie der "
   "Finger eines Mannes. Sie scheint das einzige Überbleibsel der alten Drachen "
   "zu sein, von denen man glaubt, dass sie schon vor Jahrhunderten ausgestorben "
   "seien."
bluedragonscale_name_plural_rsc = de "Drachenschuppen"
